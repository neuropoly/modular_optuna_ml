{
  "label": "IrisTesting",
  "format": "tabular",
  "data_source": "./testing/iris_data/iris_testing.tsv",
  "separator": "\t",
  "index": "id",
  "foo": "bar",
  "pre_split_hooks": [
    {
      "type": "feature_drop_null",
      "threshold": 0.5
    }, {
      "type": "sample_drop_null",
      "threshold": 0.5
    }
  ],
  "post_split_hooks": [
    {
      "type": "one_hot_encode",
      "features": ["color"]
    },
    {
      "type": "imputation_simple",
      "strategy": "mean",
      "run_per_cross": true
    }, {
      "type": "standard_scaling",
      "run_per_cross": true
    },
    {
      "type": "principal_component_analysis",
      "proportion": {
        "label": "pca_component_proportion",
        "type": "float",
        "low": 0.1,
        "high": 0.2
      }
    },
    {
      "type": "recursive_feature_elimination",
      "proportion": {
        "label": "rfe_feature_proportion",
        "type": "float",
        "low": 0.1,
        "high": 0.2
      }
    }
  ]
}